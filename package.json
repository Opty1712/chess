{
  "name": "chess-board-js-skills",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "clean": "rimraf ./.next ./.linaria-cache",
    "dev": "next",
    "start": "next start",
    "test": "cross-env NODE_ENV=test jest -c jest.config.js",
    "lint": "yarn run lint-ts && yarn lint-commitlint && yarn run lint-prettier && yarn run lint-stylelint && yarn lint-eslint",
    "lint-ts": "tsc",
    "lint-commitlint": "yarn commitlint -c commitlint.config.js --from 399c74075c502b05ae4c65f0ca58ea97d1211b14",
    "lint-eslint": "eslint --ext .js,.jsx,.ts,.tsx --color --cache",
    "lint-prettier": "prettier -c **/*",
    "lint-stylelint": "stylelint **/*.{ts,tsx}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ],
    "*.tsx": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.js": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "stylelint",
      "eslint --color --fix --cache"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@commitlint/config-conventional": "^12.1.1",
    "@testing-library/react": "^11.2.6",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-plugin-ts-nameof": "^4.2.1",
    "commitlint": "^12.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "linaria": "^2.1.0",
    "next": "^10.1.3",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.7.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-nameof": "^5.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
